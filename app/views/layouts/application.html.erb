<!DOCTYPE html>
<html>
<head>
  <title>Myapp</title>
  <%= stylesheet_link_tag    "application", media: "all", "data-turbolinks-track" => true %>
  <%= javascript_include_tag "application", "data-turbolinks-track" => true %>
  <%= csrf_meta_tags %>
</head>
<body>

	<% flash.each do |key,value| %>
      <%= value %>
	<% end %>
    
    <% if current_user %>
	<%= link_to "Logout" , logout_path, method: :delete, confirm: "CONFIRM LOGOUT" %>
	<% end %>

<%= yield %>

</body>
</html>
